ROOTDIR := ../../
PROJDIR := ../

FILENAME := msg
OUTFILE := $(FILENAME)_server
MAIN_C := $(OUTFILE).c
SERVER := $(FILENAME)_svc.c

PREREQ :=
PREREQ += $(PROJDIR)$(MAIN_C)
PREREQ += $(PROJDIR)$(SERVER)

INC :=
INC += -I$(ROOTDIR)
INC += -I$(PROJDIR)


$(OUTFILE): $(PREREQ)
#	$(MAKE) -C $(ROOTDIR)
	gcc $(INC) $(PREREQ) -o $(OUTFILE) -lnsl
